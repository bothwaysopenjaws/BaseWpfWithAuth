<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="MenuButtonColorSelected" Color="PaleVioletRed" ></SolidColorBrush>
	<SolidColorBrush x:Key="MenuButtonColor" Color="Violet" ></SolidColorBrush>
	<Style x:Key="ButtonMenu" TargetType="{x:Type Button}">
		<Style.Setters>
			<Setter Property="Background" Value="{StaticResource MenuButtonColor}"/>
		</Style.Setters>
	</Style>

</ResourceDictionary>